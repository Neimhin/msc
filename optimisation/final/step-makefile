PYTHON := python3
all-exp-steps: exp-step/1.touch exp-step/8.touch exp-step/16.touch exp-step/32.touch exp-step/64.touch exp-step/128.touch exp-step/256.touch exp-step/1024.touch exp-step/2048.touch exp-step/4096.touch

exp-step/4096.touch: exp-step/4096/0 exp-step/4096/1 exp-step/4096/2 exp-step/4096/3 exp-step/4096/4

exp-step/4096/0:
	$(PYTHON) src/step_by_step.py --run-name 0 --fold 0 --batch-size 4096
exp-step/4096/1:
	$(PYTHON) src/step_by_step.py --run-name 1 --fold 1 --batch-size 4096
exp-step/4096/2:
	$(PYTHON) src/step_by_step.py --run-name 2 --fold 2 --batch-size 4096
exp-step/4096/3:
	$(PYTHON) src/step_by_step.py --run-name 3 --fold 3 --batch-size 4096
exp-step/4096/4:
	$(PYTHON) src/step_by_step.py --run-name 4 --fold 4 --batch-size 4096

